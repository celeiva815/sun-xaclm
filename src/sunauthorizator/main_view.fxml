<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="378.0" prefWidth="317.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sunauthorizator.SunAuthorizator">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <children>
        <Button fx:id="submitButton" layoutX="27.0" layoutY="26.0" onAction="#selectXACMLRequest" prefHeight="33.0" prefWidth="327.0" text="Seleccionar XACMLRequest" />
        <Text layoutX="27.0" layoutY="81.0" text="No has seleccionado ningún xml..." fx:id="xacmlRequestText" />
        <Button layoutX="27.0" layoutY="103.0" mnemonicParsing="false" onAction="#selectXACMLPolicies" prefHeight="33.0" prefWidth="327.0" text="Seleccionar XACMLPolicies" />
        <Text layoutX="27.0" layoutY="162.0" text="No has seleccionado ningún xml..." fx:id="xacmlPoliciesText" />
        <Button layoutX="27.0" layoutY="189.0" mnemonicParsing="false" onAction="#checkAuthorization" prefHeight="43.0" prefWidth="327.0" text="Comprobar autorización" />
        <Label alignment="TOP_LEFT" layoutX="27.0" layoutY="252.0" prefHeight="17.0" prefWidth="159.0" text="Información de autorización:" underline="true" />
        <Label fx:id="informationLabel" alignment="TOP_LEFT" layoutX="27.0" layoutY="276.0" prefHeight="73.0" prefWidth="327.0" text="Sin información" />
    </children>
</AnchorPane>
